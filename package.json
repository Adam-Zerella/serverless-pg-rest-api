{
  "name": "contango",
  "version": "0.1.0",
  "private": true,
  "description": "Node.js REST API scaffold with sensible defaults and features.",
  "scripts": {
    "test": "mocha",
    "dev": "nodemon",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "build": "run-s build:*",
    "build:clean": "rimraf dist/",
    "build:js": "esbuild src/server.ts --platform=node --bundle --outdir=dist --target=node14 --external:knex --external:pg --minify --legal-comments=none",
    "db": "run-s db:*",
    "db:rollback": "knex migrate:rollback --all",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run"
  },
  "keywords": [
    "contango",
    "functional",
    "scaffold",
    "template",
    "framework",
    "nodejs"
  ],
  "engines": {
    "node": "~14"
  },
  "author": "Adam Zerella <hi@azerel.la> (https://azerel.la)",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.3",
    "@types/koa__cors": "^3.0.3",
    "@types/koa__router": "^8.0.8",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "@types/ramda": "^0.27.64",
    "@types/sinon": "^10.0.6",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "chai": "^4.3.4",
    "esbuild": "^0.14.19",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^7.5.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "dotenv": "^10.0.0",
    "knex": "^0.95.11",
    "koa": "^2.13.3",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "pg": "^8.7.1",
    "pino": "^7.0.2",
    "ramda": "^0.28.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  }
}
